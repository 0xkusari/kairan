<h2><%= @channel.title %></h2>
<% if @channel.icon_url %>
  <p><%= image_tag @channel.icon_url, size: "200x200" %></p>
<% end %>
<p><%= link_to @channel.url, @channel.url %></p>
<h3><%= @channel.items.count %> items</h3>
<ul>
  <% @items.each do |item| %>
    <li><%= link_to item.title, item.url %></li>
  <% end %>
</ul>
